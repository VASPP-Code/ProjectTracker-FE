
{
	"_version": "1.42.0",
	"sap.app": {
	  "id": "VASPP.employee.view.FlexibleColumnLayout",
	  "type": "application",
	  "i18n": "i18n/i18n.properties",
	  "applicationVersion": {
		"version": "0.0.1"
	  },
	  "title": "{{appTitle}}",
	  "description": "{{appDescription}}",
	  "resources": "resources.json",
	  "sourceTemplate": {
		"id": "@sap/generator-fiori:basic",
		"version": "1.8.0",
		"toolsId": "083007b7-a859-4eac-bbfc-333db91db9f7"
	  },
	  "dataSources": {
		"mainService": {
		  "uri": "/sap/opu/odata/",
		  "type": "OData",
		  "settings": {
			"annotations": [],
			"localUri": "localService/metadata.xml",
			"odataVersion": "2.0"
		  }
		}
	  }
	},
	"sap.ui5": {
		"flexEnabled": true,
        "rootView": {
			"viewName": "VASPP.employee.view.FlexibleColumnLayout",
			"type": "XML",
			"async": true,
			"id": "fcl"
		},
		
		"dependencies": {
			"minUI5Version": "1.98.0",
			"libs": {
				"sap.ui.core": {
				},
				"sap.m": {
				},
				"sap.f": {
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		  },
		
		"config": {
			"fullWidth": true
		},
		"routing": {
			"config": {
				"routerClass": "sap.f.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "VASPP.employee.view",
				"controlId": "fcl",
				"transition": "slide",
				"async": true
			},
			
			"routes": [
				{
					"pattern": "page2",
					"name": "page2",
					"target": "page2",
					"layout": "EndColumnFullScreen"
				},
				{
					"pattern": ":layout:",
					"name": "master",
					"target": [
						"master"
					]
				},
				
				{
					"pattern": "detail/{product}/{layout}",
					"name": "detail",
					"target": [
						"master",
						"detail"
					]
				},
				{
					"pattern": "AddNewEmployee/{AddCust}/{layout}/{listindex}",
					"name": "AddNewEmployee",
					"target": [
						"AddNewEmployee"
					]
				}
				
			],
			"targets": {
				"master": {
					"name": "Master",
					"id":"master1",
					"controlAggregation": "beginColumnPages"
				},
				"detail": {
					"name": "Detail",
					"id":"detail1",
					"controlAggregation": "midColumnPages"
				},
				"AddNewEmployee": {
					"name": "AddNewEmployee",
					"id":"AddNewEmployee1",
					"controlAggregation": "beginColumnPages"
				}
			}
		},
		"models": {
			"i18n": {
			  "type": "sap.ui.model.resource.ResourceModel",
			  "settings": {
				"bundleName": "vaspp.employee.i18n.i18n"
			  				}
					},
					"memployee":{
						"type": "sap.ui.model.json.JSONModel",
						"uri": "model/employeedetails.json"
					  },
					  "mapplications":{
						"type": "sap.ui.model.json.JSONModel",
						"uri": "model/applications.json"
					  },
					
					  
			"": {
			  "dataSource": "mainService",
			  "preload": true,
			  "settings": {}
			}
		  }
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
		  "icon": "",
		  "favIcon": "",
		  "phone": "",
		  "phone@2": "",
		  "tablet": "",
		  "tablet@2": ""
		},
		"deviceTypes": {
		  "desktop": true,
		  "tablet": true,
		  "phone": true
		}
	  }
}